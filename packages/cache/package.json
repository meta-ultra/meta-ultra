    "file-loader": "^6.2.0",
    "hoist-non-react-statics": "^3.3.2",
    "html-entities": "^2.3.6",
    "html-webpack-plugin": "^5.5.3",
    "http-proxy-middleware": "^2.0.6",
    "jest": "^29.7.0",
