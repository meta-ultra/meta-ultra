{
  "name": "@meta-ultra/cache",
  "version": "0.1.8",
  "description": "Meta Ultra Cache",
  "keywords": [
    "react",
    "cache",
    "Meta Ultra"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "test": "jest",
    "build": "pnpm clean && pnpm run \"/^build:.*/\"",
    "build:cjs": "webpack & pnpm copy:cjs",
    "build:types": "tsc -P ./tsconfig.types.full.json & tsc -P ./tsconfig.types.core.json & pnpm copy:types",
    "copy:cjs": "cpy ./src/__dist__/cjs/cache.{full,core}.js ./dist/cjs/ --flat",
    "copy:types": "cpy ./src/__dist__/cjs/cache.{full,core}.d.ts ./dist/cjs/ --flat"
  },
  "author": "fsjohnhuang",
  "license": "MIT",
  "typings": "./dist/cjs/cache.full.d.ts",
  "exports": {
    ".": "./dist/cjs/cache.full.js",
    "./dist/cjs/cache.core": "./dist/cjs/cache.core.js"
  },
  "files": [
    "dist/**/*"
  ],
  "sideEffects": false,
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/plugin-transform-react-constant-elements": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "@babel/preset-typescript": "^7.22.5",
    "@babel/runtime-corejs3": "^7.22.5",
    "@svgr/webpack": "^8.0.1",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.5",
    "@types/lodash-es": "^4.17.7",
    "@types/node": "^20.2.5",
    "@types/react": "^18.2.9",
    "@types/react-document-title": "^2.0.5",
    "@types/react-dom": "^18.2.5",
    "@types/react-is": "^18.2.1",
    "@types/webpack-env": "^1.18.1",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "ansi-html-community": "^0.0.8",
    "babel-loader": "^9.1.2",
    "copy-webpack-plugin": "^11.0.0",
    "cpy-cli": "^5.0.0",
